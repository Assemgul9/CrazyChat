<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.ListView?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.demo.RegController" stylesheets="@style.css">
        <Label text="Login"></Label>
        <TextField fx:id="loginField" HBox.hgrow="ALWAYS" promptText="login"/>
        <Label text="Password"></Label>
        <PasswordField fx:id="passwordField" HBox.hgrow="ALWAYS" promptText="password"></PasswordField>
        <Label text="Nickname"></Label>
        <TextField fx:id="nicknameField" HBox.hgrow="ALWAYS" promptText="nickname"/>
        <TextArea fx:id="textArea" VBox.vgrow="ALWAYS"  editable="false"  />

        <Button text="Reg" onAction="#clickButtonReg" alignment="BOTTOM_CENTER"></Button>


</VBox>
